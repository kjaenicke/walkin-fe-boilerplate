    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.0.1",
    "antd": "^4.0.2",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "husky": "^4.2.3",
